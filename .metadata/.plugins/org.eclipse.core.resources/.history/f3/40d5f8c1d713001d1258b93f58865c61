package Module3;
import java.util.*;
public class dibya {
	public static void main(String args[]) {
		int a,b;
		char[] d;
 		String s= new String();
		String t= new String();
		Scanner sc = new Scanner(System.in);
		s=sc.nextLine();
		//d=s.toCharArray();
		a=sc.nextInt();
		b=sc.nextInt();
		t=s.substring(s.length()-a-1,s.length());
		s=t+s.substring(0,s.length()-a-1);
		t=s.substring(s.length()-b-1,s.length());
		s=t+s.substring(0,s.length()-b-1);
		
		System.out.println(s);
	
	}
}
