package Module3;
import java.util.*;
//import java.util.
public class dibya {
	public static void main(String args[]) {
		int a,b,count=0;
		char[] d;
 		String s= new String();
		String t= new String();
		String r= new String(s);
		Scanner sc = new Scanner(System.in);
		s=sc.nextLine();
		//d=s.toCharArray();
		a=sc.nextInt();
		b=sc.nextInt();
		do{
		if(a!=0) {
		t=s.substring(s.length()-a-1,s.length());
		s=t+s.substring(0,s.length()-a-1);
		}
		if(b!=0) {
		t=s.substring(s.length()-b-1,s.length());
		s=t+s.substring(0,s.length()-b-1);
		}count++;
		}while(r.equals(s));
		System.out.println(count);
	
	}
}
